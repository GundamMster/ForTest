@model IEnumerable<WebApplication1.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}
<p>
    @Html.ActionLink("Create", "game", "create")
</p>

<h2>Players</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>

    </tr>

    @foreach (var user in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => user.Email)
            </td>

            <td>
                @Html.ActionLink("Start", "Create", "Game", new { id = user.Id }, new { })
            </td>
        </tr>
    }

</table>
